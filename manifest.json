{
    "manifest_version": 3,
    "name": "Moralis Crypto Charts",
    "version": "1.0",
    "description": "Live crypto candlestick charts for any token address. Real-time price data for EVM chains and Solana",
    "permissions": [
      "contextMenus",
      "storage"
    ],
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
}