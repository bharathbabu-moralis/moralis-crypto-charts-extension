{
    "manifest_version": 3,
    "name": "Token Charts",
    "version": "1.0",
    "description": "View token charts directly from addresses",
    "permissions": [
      "contextMenus",
      "storage"
    ],
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "32": "icon32.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }